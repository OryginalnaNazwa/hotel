<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="269.0" prefWidth="325.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.hotel.UserController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="labelID" text="Hotel Systems" />
    <HBox alignment="CENTER" prefHeight="47.0" prefWidth="286.0" spacing="20.0">
        <children>
            <Button mnemonicParsing="false"  text="Make reservations" />
            <Button mnemonicParsing="false"  text="Edit user" />
            <Button mnemonicParsing="false" onAction="#onLogOutButtonClick" text="Log out" />
        </children></HBox>
</VBox>
